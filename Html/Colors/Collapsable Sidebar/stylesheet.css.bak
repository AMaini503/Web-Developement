body {
	margin: 0px;
	padding: 0px;
	border: 0px;
}
.navbar {
	overflow: auto;
	position: fixed;
	z-index: 1;
	width: 100%;
	height: 50px;
	top: 0px;
	border-bottom: 1px solid #cccccc;
	background-color: #B404AE;
	left: 0px;
}
.navbar ul {
	padding: 0px;
	margin-top: 15px;
}
.navbar ul:nth-child(1) {
	margin-left: 5px;
}
.navbar ul:nth-child(2) {
	margin-right: 5px;
}
.navbar li {
	display: inline;
	padding: 10px;
}
.navbar ul li a {
	text-decoration: none;
	color: #cccccc;

}
.pull-right {
	float: right;
}
.pull-left {
	float: left;
}

.container {
	margin-top: 50px;
	padding: 20px;
	width: auto;
	display: inline-block;
	background-color: black;
	color: white;
	position: relative;
}
.side-navbar {
	display: inline-block;
	background-color: black;
	margin-top: 49px;
	padding-top: 20px; 
	position: fixed;
	aopacity: 1;
	right: 0;
	top: 0;
}
.snavbar-item {
	color: white;
	padding: 20px;
	background-color: #cccccc;
}
.snavbar-item a {
	text-decoration: none;
	color: white;
}
