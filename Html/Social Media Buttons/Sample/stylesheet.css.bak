.socialfloat {
	position: fixed;
	left:0;
	top: 200px;
	z-index: 9999;
	display: block;
	width: 100%;
}

.socialfloat .btnz {
	width:50px;
	height: 50px;
	color: #fff;
	text-align: center;
	display: block;
}
.socialfloat .btnz i {
	font-size: 30px;
	line-height: 50px;
}
.facebook {
    background-color: #3b5998;
}
 
.gplus {
    background-color: #dd4b39;
}
 
.twitter {
    background-color: #55acee;
}
 
.stumbleupon {
    background-color: #eb4924;
}
 
.pinterest {
    background-color: #cc2127;
}
 
.linkedin {
    background-color: #0077b5;
}
 
.buffer {
    background-color: #323b43;
}
